from building import *
src = []
cwd     = GetCurrentDir()
CPPPATH = [cwd + '/include',cwd + '/sdhci-platform']
src += Glob('*.c')
src += Glob('src/*.c')
src += Glob('sdhci-platform/*.c')

group = DefineGroup('Drivers', src, depend = ['RT_USING_SDHCI'], CPPPATH = CPPPATH)

objs = [group]

Return('objs')