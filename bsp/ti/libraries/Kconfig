
choice
    prompt "TI SOC"
    default SOC_AM335X
    help
        Select the TI SOC

    config SOC_AM335X
        bool "TI AM335x"
        select RT_USING_COMPONENTS_INIT
        select RT_USING_USER_MAIN
endchoice

menu "On-chip Peripheral Drivers"

config RT_USING_UART0
    bool "Using RT_USING_UART0"
    default n

config RT_USING_UART1
    bool "Using RT_USING_UART1"
    default n

endmenu

