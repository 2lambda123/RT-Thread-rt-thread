{
    "C_Cpp.dimInactiveRegions": false,
    "files.exclude": {
        "**/.git": true,
        "**/GPUCache" : true,
        "**/build" : true,
        "**/*.pyc" : true
    },
    "editor.renderWhitespace": "all",
    "MicroPython.executeButton": [
        {
            "text": "▶",
            "tooltip": "运行",
            "alignment": "left",
            "command": "extension.executeFile",
            "priority": 3.5
        }
    ],
    "MicroPython.syncButton": [
        {
            "text": "$(sync)",
            "tooltip": "同步",
            "alignment": "left",
            "command": "extension.execute",
            "priority": 4
        }
    ]
}
